{
  "license": "MIT",
  "name": "@zefiros/npm-defaults",
  "version": "0.2.0-beta.7",
  "bin": {
    "npm-defaults": "./bin/run"
  },
  "oclif": {
    "bin": "npm-defaults",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "npm-defaults": {
    "type": "oclif-cli",
    "skipTemplate": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zefiros-Software/npm-defaults.git"
  },
  "description": "Build tools for Zefiros packages",
  "files": [
    ".huskyrc.js",
    "oclif.manifest.json",
    "commitlint.config.js",
    "jest.config.js",
    "package.json",
    "prettier.config.js",
    "release.config.js",
    "tsconfig.base.json",
    "tslint.json",
    "wallaby.base.js",
    "bin",
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "yarn ttsc -p tsconfig.dist.json",
    "test": "yarn ttsc -p tsconfig.lint.json && jest test",
    "fix": "yarn lint --fix",
    "lint": "tslint --project tsconfig.lint.json",
    "release": "yarn semantic-release",
    "release:dry": "yarn release --dry-run",
    "package": "rm -rf dist && yarn build",
    "prepack": "yarn ts-node -r tsconfig-paths/register node_modules/@oclif/dev-cli/bin/run manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json"
  },
  "dependencies": {
    "@actions/core": "^1.1.3",
    "@actions/github": "^1.1.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-plugins": "^1.7.8",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.1.0-beta.3",
    "@types/jest": "^24.0.19",
    "@types/prettier": "^1.18.3",
    "@zerollup/ts-transform-paths": "^1.7.3",
    "conventional-changelog": "^3.1.12",
    "execa": "^3.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "line-diff": "^2.1.0",
    "lint-staged": "^9.4.2",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "semantic-release": "^16.0.0-beta.24",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-no-circular-imports": "^0.7.0",
    "tslint-no-commented-code-rule": "^0.1.0",
    "tslint-plugin-prettier": "^2.0.1",
    "ttypescript": "^1.5.7",
    "typescript": "^3.7.0-beta",
    "typestrict": "^1.0.2",
    "variable-diff": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "package-json": "^6.5.0"
  }
}